#include<stdio.h>
int main(){
int i,n,flag=0,key,a[10];
printf("enter the no.of elements to insert");
scanf("%d",&n);
printf("enter the array elements");
for(int i=0;i<n;i++){
scanf("%d",&a[i]);
}
printf("enter the key value");
scanf("%d",&key);
for(int i=0;i<n;i++){
if(key==a[i]){
printf("element found");
}}
if(key==0){
printf("element not foud");
}
return 0;
}
